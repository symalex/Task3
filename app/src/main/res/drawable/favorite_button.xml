<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
	<item android:state_pressed="false">
		<bitmap android:src="@android:drawable/btn_star_big_off" />
		<shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle">
			<solid android:color="#eeffffff" />
			<corners android:bottomLeftRadius="24dp" android:bottomRightRadius="24dp" android:topLeftRadius="24dp" android:topRightRadius="24dp" />
		</shape>
	</item>
	<item android:state_pressed="true">
		<bitmap android:src="@android:drawable/btn_star_big_on" />
		<shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle">
			<solid android:color="@color/colorAccent" />
			<corners android:bottomLeftRadius="24dp" android:bottomRightRadius="24dp" android:topLeftRadius="24dp" android:topRightRadius="24dp" />
		</shape>
	</item>
</selector>
